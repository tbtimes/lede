{
  "name": "lede",
  "version": "2.0.29",
  "description": "A tool for building and deploying static article pages.",
  "repository": "https://github.com/tbtimes/lede",
  "main": "dist/index.js",
  "typings": "defs/index.d.ts",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/interfaces"
    ],
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/__tests__/fixtures"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest --coverage --no-cache",
    "dev": "typings install && gulp dev"
  },
  "author": "Tampa Bay Times Data Team",
  "license": "MIT",
  "engines": {
    "node": "^6.3.0"
  },
  "dependencies": {
    "archieml": "^0.3.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "csso": "^2.3.0",
    "glob-promise": "^1.0.6",
    "node-sass": "^3.13.1",
    "nunjucks": "^2.4.2",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-includepaths": "0.1.6",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sander": "^0.5.1",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "@types/bunyan": "0.0.30",
    "@types/node": "^6.0.42",
    "@types/node-sass": "0.0.29",
    "@types/nunjucks": "0.0.32",
    "bunyan": "^1.8.1",
    "chokidar": "^1.6.1",
    "coveralls": "^2.11.11",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.5.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.0.2",
    "jest": "^18.1.0",
    "jest-cli": "^17.0.3",
    "merge2": "^1.0.2",
    "nock": "^8.0.0",
    "nyc": "^7.0.0",
    "rimraf": "^2.5.4",
    "ts-jest": "^17.0.3",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.5"
  }
}
